{
  "compilerOptions": {
    "incremental": true,
    "target": "es2022",
    "module": "node16",
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
}
